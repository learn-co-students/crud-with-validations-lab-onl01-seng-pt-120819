<h1>Create Your Song</h1>

<%= form_for @song do |f|%>

    <%= f.label "Title"%>
    <%= f.text_field :title%>

    <%= f.label "Artist Name"%>
    <%= f.text_field :artist_name%>

    <%= f.label "Genre"%>
    <%= f.text_field :genre%>

    <%= f.label "Release Year"%>
    <%= f.text_field :release_year%>

    <%= f.label "Released"%>
    <%= f.text_field :released%>

    <%= f. submit "Create"%>

<% end %>